@model IEnumerable<Vidly.Models.Movies>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2> Hawa Index</h2>


<table class="table table-hover">


    <thead>
    <thead>
        <tr>
            <th scope="col">Movies</th>
            <th scope="col">Genre</th>
            <th scope="col">Action</th>

        </tr>
    </thead>
    <tbody>
        @foreach(var movies in Model)
        {
            <tr>
                <td>@movies.Name</td>
                
                <td></td>
            </tr>
        }

    </tbody>


</table>